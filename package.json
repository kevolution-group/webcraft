{
  "name": "@kevolution/webcraft",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "pnpm run clear:dist && pnpm run tsc && vite build -c vite.config-lib.ts",
    "build:docs": "pnpm run clear:dist && pnpm run tsc:docs && vite build -c vite.config.ts",
    "clear:dist": "rimraf dist",
    "dev": "vite",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "pnpm run build",
    "preview": "vite preview -c vite.config.ts",
    "tsc": "tsc --p ./tsconfig.build.json",
    "tsc:docs": "tsc --p ./tsconfig.json"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "eslint": "8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "glob": "10.3.16",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "5.0.7",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-dts": "3.9.1",
    "vite-plugin-lib-inject-css": "2.1.1"
  }
}
